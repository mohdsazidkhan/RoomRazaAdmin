{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/server.js",   
        "use": "@vercel/static"
      }
    ],
 
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.js"
      }
    ]
  }